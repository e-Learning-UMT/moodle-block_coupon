    </TABLE>
    <TABLE NAME="voucher_groups" COMMENT="voucher_groups table retrofitted from MySQL" PREVIOUS="voucher_cohorts">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="18" NOTNULL="true" SEQUENCE="true" NEXT="voucherid"/>
