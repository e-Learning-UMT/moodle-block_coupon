
<XMLDB PATH="blocks/voucher/db" VERSION="20120406" COMMENT="XMLDB file for Moodle blocks/voucher"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
	<TABLE NAME="voucher_table" COMMENT="voucher_table table retrofitted from MySQL">
	    <FIELDS>
		<FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="created_on"/>
		<FIELD NAME="created_on" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="created_by"/>
